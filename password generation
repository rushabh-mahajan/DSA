#include<iostream>
using namespace std;
class Pswd
{  int i,j,k,l,count;
    char a[6];
    public:
    void getdata();
    void gen();
 };
  void Pswd::getdata()
  {  cout<<"enter the six character";
      for(i=0;i<6;i++)
      cin>>a[i];
     
    }  
    void Pswd::gen()
    {   count=0;
         for(i=0;i<6;i++)
         {   
              for(j=0;j<6;j++)
              { 
                    for(k=0;k<6;k++)
                    { 
                               for(l=0;l<6;l++)
                               {   cout<<a[i]<<a[j]<<a[k]<<a[l]<<"\n";     count++;
                                  
                               }    
                     }             
                }
           }      cout<<"\ntotal no. of possible password\n"<<count<<"\n";
      }          
 int main()
 {     Pswd p;
         p.getdata();
         cout<<"\npossible passwords are\n";
         p.gen();
         return 0;
  }
